AA,EAAA;AACA,gCAAA,KAAA;AACA,KAdA;AAeA,IAAA,SAfA,uBAeA;AACA,WAAA,OAAA,CAAA,IAAA,CAAA;AAAA,QAAA,KAAA,EAAA,EAAA;AAAA,QAAA,WAAA,EAAA,EAAA;AAAA,QAAA,OAAA,EAAA;AAAA,OAAA;AACA,KAjBA;AAkBA,IAAA,YAlBA,wBAkBA,KAlBA,EAkBA;AACA,WAAA,OAAA,CAAA,MAAA,CAAA,KAAA,EAAA,CAAA;AACA;AApBA;AApBA,CAAA","sourcesContent":["<template>\n  <section class=\"page-section\">\n    <b-container>\n      <HeaderPage title=\"Adicionar Questão\"/>\n\n      <!--FORM-->\n      <b-row>\n        <b-col cols=\"2\"></b-col>\n        <b-col cols=\"8\">\n          <form @submit.prevent=\"add\">\n            <div class=\"form-group\">\n              <input\n                v-model=\"question\"\n                type=\"text\"\n                class=\"form-control\"\n                id=\"txtName\"\n                placeholder=\"escreve a questão\"\n                required\n              />\n            </div>\n            <div class=\"form-group\">\n              <textarea\n                id=\"txtDescription\"\n                class=\"form-control\"\n                placeholder=\"escreve uma descrição\"\n                cols=\"30\"\n                rows=\"10\"\n                v-model=\"description\"\n                required\n              ></textarea>\n            </div>\n            <div class=\"form-group\">\n              <input\n                v-model=\"level\"\n                type=\"text\"                \n                min=\"1\"\n                max=\"5\"\n                onmouseenter=\"(this.type='number')\"\n                onmouseleave=\"(this.type='text')\"                \n                class=\"form-control\"\n                id=\"txtLevel\"\n                placeholder=\"indica o nível da questão\"\n                required\n              />\n            </div>\n            <div v-if=\"answers.length\">\n              <div class=\"row\">\n                <div class=\"col-md-7\">\n                  <h4>Lista de respostas</h4>\n                </div>\n                <div class=\"col-md-2\">\n                  <h4>Correta</h4>\n                </div>\n                <div class=\"col-md-3\">\n                  <h4>Ações</h4>\n                </div>\n              </div>\n            </div>\n            <div v-for=\"(answer, index) in answers\" :key=\"answer.title\">\n              <div class=\"form-group\">\n                <div class=\"row\">\n                  <div class=\"col-md-7\">\n                    <input\n                      v-model.lazy=\"answer.title\"\n                      type=\"text\"\n                      class=\"form-control\"\n                      id='txtAnswer'\n                      :placeholder=\"setPlaceHolder(index+1)\"\n                      \n                    />\n                  </div>\n                  <div class=\"col-md-2\">\n                    <input\n                      v-model=\"answer.correct\"\n                      type=\"checkbox\"\n                      class=\"form-control\"\n                      id=\"chkAnswer\"\n                    />\n                  </div>\n                  <div class=\"col-md-3\">\n                    <button\n                      @click=\"removeAnswer(index)\"\n                      type=\"button\"\n                      class=\"btn btn-outline-danger mr-2\"\n                    ><i class=\"fas fa-trash\"></i> REMOVER</button>\n                  </div>\n                </div>\n              </div>\n            </div>\n            <button\n              @click=\"addAnswer\"\n              type=\"button\"\n              class=\"btn btn-outline-success mr-2\"\n            ><i class=\"fas fa-plus-square\"></i> ADICIONAR RESPOSTAS</button>\n            <!--<pre>{{ $data }}</pre>-->\n            <button type=\"submit\" class=\"btn btn-outline-success mr-2\"><i class=\"fas fa-save\"></i> GRAVAR QUESTÃO</button>\n            <router-link\n              :to=\"{name: 'listQuestions'}\"\n              tag=\"button\"\n              class=\"btn btn-outline-danger\"\n            ><i class=\"fas fa-window-close\"></i> CANCELAR</router-link>\n          </form>\n        </b-col>\n        <b-col cols=\"2\"></b-col>\n      </b-row>\n    </b-container>\n    <pre>{{$data}}</pre>\n  </section>\n</template>\n\n<script>\nimport { ADD_QUESTION } from \"@/store/questions/question.constants\";\nimport router from \"@/router\";\nimport HeaderPage from \"@/components/HeaderPage.vue\"\nimport { mapGetters } from \"vuex\";\n\nexport default {\n  name: \"AddQuestion\",\n   components: {\n    HeaderPage\n  },\n  data: () => {\n    return {\n      title: \"\",\n      group: \"\",\n      type: \"Escolha múltipla\",\n      question: \"\",\n      description: \"\",\n      level: \"\",\n      active: true,\n      answers: []\n    };\n  },\n  computed: {\n    ...mapGetters(\"question\", [\"getMessage\"])\n  },\n  methods: {\n    add() {\n      this.$store.dispatch(`question/${ADD_QUESTION}`, this.$data).then(\n        () => {\n          this.$alert(this.getMessage, \"Questão adicionada!\", \"success\");\n          router.push({name: 'listQuestions'});\n        },\n        err => {\n          this.$alert(`${err.message}`, \"Erro\", \"error\");\n        }\n      );\n    },    \n    setPlaceHolder: index => {\n      return `insira a ${index}ª resposta`;\n    },\n    addAnswer() {\n      this.answers.push({ title: \"\", description:\"\", correct: false });\n    },\n    removeAnswer(index) {\n      this.answers.splice(index, 1);\n    },\n  }\n};\n</script>"],"sourceRoot":"src/views/questions"}]}