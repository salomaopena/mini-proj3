\"Lista de Quizzes\" />\n\n      <b-row>\n        <QuizBox\n          v-for=\"(quiz, index) in quizzes\"\n          :key=\"quiz._id\"\n          :idQuiz=\"quiz._id\"\n          :index=\"index+1\"\n          :title=\"quiz.name\"\n        />\n      </b-row>\n    </b-container>\n  </section>\n</template>\n<script>\nimport { FETCH_QUIZZES } from \"@/store/quizzes/quiz.constants\";\nimport HeaderPage from \"@/components/HeaderPage.vue\";\nimport QuizBox from \"@/components/QuizBox.vue\";\nimport { mapGetters } from \"vuex\";\n\nexport default {\n  components: {\n    HeaderPage,\n    QuizBox\n  },\n   data: function() {\n    return {\n      quizzes: []\n    };\n  },\n  computed: {\n    ...mapGetters(\"quiz\", [\"getQuizzes\"])\n  },\n  methods: {\n    fetchQuizzes() {\n      this.$store\n      .dispatch(`quiz/${FETCH_QUIZZES}`)\n      .then(\n        () => {\n          this.quizzes = this.getQuizzes\n          this.quizzes.sort(this.compareLevels)\n        },\n        err => this.$alert(`${err.message}`, \"Erro\", \"error\")\n      );\n    },\ncompareLevels(q1, q2) {\n      if (q1.level > q2.level) return 1;\n      if (q1.level < q2.level) return -1;\n      else return 0;\n    }\n  },\n  created() {\n    this.fetchQuizzes()\n  }\n};\n</script>"],"sourceRoot":"src/views"}]}