ble>\n        </b-col>\n        <b-col cols=\"2\"></b-col>\n      </b-row>\n    </b-container>\n  </section>\n</template>\n\n<script>\nimport HeaderPage from \"@/components/HeaderPage.vue\";\nimport { mapGetters } from \"vuex\";\n\nexport default {\n  name: \"Managesponsors\",\n  components: {\n    HeaderPage,\n  },\n  data: function () {\n    return {\n      sponsors: [],\n      sortType: 1,\n    };\n  },\n  computed: {\n    ...mapGetters(\"sponsor\", [\"getSponsor\", \"getMessage\"]),\n  },\n};\n</script>"]}]}